{
  "name": "promised-location",
  "version": "1.0.0",
  "description": "promisified geolocation library",
  "main": "index.js",
  "scripts": {
    "build": "babel -d dist/ src/",
    "coverage": "nyc check-coverage --lines 80 --functions 80 --branches 75",
    "fix": "npm run fix-src && npm run fix-test",
    "fix-src": "xo --fix src/**/*",
    "fix-test": "xo --fix test/**/*",
    "instrument": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint": "npm run lint-src && npm run lint-test",
    "lint-src": "xo src/**/*",
    "lint-test": "xo test/**/*",
    "prepublish": "npm run build",
    "report": "nyc report",
    "test": "npm run build && ava --tap | tap-nyan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vinniegarcia/promised-location.git"
  },
  "keywords": [
    "promise",
    "geolocation"
  ],
  "author": "vinnie garcia <vinnie@coev.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinniegarcia/promised-location/issues"
  },
  "homepage": "https://github.com/vinniegarcia/promised-location",
  "dependencies": {
  },
  "devDependencies": {
    "ava": "^0.10.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "es6-promise": "^3.0.2",
    "nyc": "^5.3.0",
    "tap-nyan": "0.0.2",
    "xo": "^0.12.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "failFast": true,
    "serial": false,
    "tap": true,
    "verbose": false,
    "require": [
      "babel-core/register"
    ]
  },
  "xo": {
    "esnext": true,
    "envs": ["node", "browser", "es6"]
  }
}
